{
    "manifest_version": 3,
    "name": "Random Password Generator",
    "version": "1.0",
    "description": "Bing extension for generating random passwords.",
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    },
    "permissions": ["clipboardWrite"]
}
